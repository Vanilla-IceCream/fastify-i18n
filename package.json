{
  "name": "fastify-i18n",
  "version": "1.1.0",
  "description": "Internationalization plugin for Fastify. Built upon node-polyglot.",
  "main": "./dist/fastify-i18n.cjs",
  "module": "./dist/fastify-i18n.js",
  "types": "./dist/fastify-i18n.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/fastify-i18n.js",
      "require": "./dist/fastify-i18n.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vanilla-IceCream/fastify-i18n.git"
  },
  "keywords": [
    "fastify",
    "i18n",
    "l10n",
    "translation"
  ],
  "author": "Vanilla IceCream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vanilla-IceCream/fastify-i18n/issues"
  },
  "homepage": "https://github.com/Vanilla-IceCream/fastify-i18n#readme",
  "dependencies": {
    "fastify-plugin": "^4.4.0",
    "node-polyglot": "^2.4.2"
  },
  "devDependencies": {
    "@types/node-polyglot": "^2.4.2",
    "@vitest/coverage-c8": "^0.26.3",
    "fastify": "^4.11.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.26.3"
  }
}
