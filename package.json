{
  "name": "fastify-i18n",
  "version": "2.0.0",
  "description": "Internationalization plugin for Fastify. Built upon node-polyglot.",
  "main": "./dist/fastify-i18n.cjs",
  "module": "./dist/fastify-i18n.js",
  "types": "./dist/src/fastify-i18n.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/fastify-i18n.d.ts",
        "default": "./dist/fastify-i18n.js"
      },
      "require": {
        "types": "./dist/src/fastify-i18n.d.ts",
        "default": "./dist/fastify-i18n.cjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vanilla-IceCream/fastify-i18n.git"
  },
  "keywords": [
    "fastify",
    "i18n",
    "l10n",
    "translation"
  ],
  "author": "Vanilla IceCream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vanilla-IceCream/fastify-i18n/issues"
  },
  "homepage": "https://github.com/Vanilla-IceCream/fastify-i18n#readme",
  "dependencies": {
    "fastify-plugin": "^4.5.1",
    "node-polyglot": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/node-polyglot": "^2.5.0",
    "@vitest/coverage-v8": "^1.5.0",
    "fastify": "^4.26.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.2",
    "vitest": "^1.5.0"
  }
}
