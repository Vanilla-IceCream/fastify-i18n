{
  "name": "fastify-i18n",
  "version": "1.0.0",
  "description": "Internationalization plugin for Fastify. Built upon node-polyglot.",
  "main": "./dist/fastify-i18n.umd.cjs",
  "module": "./dist/fastify-i18n.js",
  "types": "./dist/fastify-i18n.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/fastify-i18n.js",
      "require": "./dist/fastify-i18n.umd.cjs"
    }
  },
  "scripts": {
    "test": "vitest --coverage",
    "build": "vite build",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vanilla-IceCream/fastify-i18n.git"
  },
  "keywords": [
    "fastify",
    "i18n",
    "internationalization"
  ],
  "author": "Vanilla IceCream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vanilla-IceCream/fastify-i18n/issues"
  },
  "homepage": "https://github.com/Vanilla-IceCream/fastify-i18n#readme",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.23.4",
    "fastify": "^4.9.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.5.0",
    "vitest": "^0.23.1"
  },
  "dependencies": {
    "fastify-plugin": "^4.3.0",
    "node-polyglot": "^2.4.2"
  }
}
